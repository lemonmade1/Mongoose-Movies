<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th> Full Name </th>
      <th> Departure <br> Date </th>
      <th> Details </th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(f) { %>
      <tr>
        <td> <%= f.fullName %> </td>
        <td> <%= f.departs %> </td>
        <td><a href="/flights/<%= f._id %>"> DETAILS </a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
